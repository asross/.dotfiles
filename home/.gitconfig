[user]
  name = Andrew Ross
  email = andrew@vermonster.com
[alias]
  st = status
  br = branch
  ci = commit
  co = checkout
  lg = log --graph --oneline --all --decorate
[diff]
  algorithm=patience
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color]
  ui = auto
